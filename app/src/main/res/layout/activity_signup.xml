<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/back"
    tools:context=".Signup">



    <ImageView
        android:id="@+id/shap"
        android:layout_width="336dp"
        android:layout_height="588dp"
        android:layout_gravity="left|start"
        android:scaleType="fitXY"
        app:srcCompat="@drawable/shap" />

    <ImageView
        android:id="@+id/drblur"
        android:layout_width="wrap_content"
        android:layout_height="444dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentBottom="true"
        android:layout_gravity="center_horizontal|bottom"
        android:layout_marginStart="0dp"
        android:layout_marginBottom="0dp"
        android:scaleType="centerCrop"
        app:srcCompat="@drawable/blur" />



    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:orientation="vertical">

        <ImageView
            android:id="@+id/imageView5"
            android:layout_width="362dp"
            android:layout_height="537dp"
            app:srcCompat="@drawable/cardview" />

    </LinearLayout>

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="83dp"
        android:textSize="30dp"
        android:fontFamily="sans-serif"
        android:textStyle="bold"
        android:textColor="@color/colorAccent"
        android:text="Sign Up" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="77dp"
        android:layout_marginTop="143dp"
        android:fontFamily="sans-serif"
        android:text="contact us"
        android:textColor="@color/colorAccent"
        android:textSize="22dp"
        android:textStyle="normal" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="50dp"
        android:layout_height="43dp"
        android:layout_below="@+id/textView6"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="12dp"
        android:layout_marginEnd="137dp"
        app:srcCompat="@drawable/facebook" />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="58dp"
        android:layout_height="43dp"
        android:layout_alignTop="@+id/drblur"
        android:layout_alignEnd="@+id/shap"
        android:layout_marginTop="14dp"
        android:layout_marginEnd="15dp"
        app:srcCompat="@drawable/gmail" />

    <EditText
        android:id="@+id/editText3"
        android:layout_width="269dp"
        android:layout_height="39dp"
        android:layout_alignStart="@+id/editText"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="4dp"
        android:layout_marginBottom="264dp"
        android:background="@drawable/budesign"
        android:ems="10"
        android:hint="Email"
        android:inputType="textEmailAddress"
        android:paddingLeft="20dp"
        android:paddingBottom="4dp" />

    <EditText
        android:id="@+id/editText"
        android:layout_width="269dp"
        android:layout_height="39dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="64dp"
        android:layout_marginTop="213dp"
        android:background="@drawable/budesign"
        android:ems="10"
        android:hint="Name"
        android:inputType="textPersonName"
        android:paddingLeft="20dp"
        android:paddingBottom="4dp" />

    <EditText
        android:id="@+id/editText4"
        android:layout_width="269dp"
        android:layout_height="39dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="64dp"
        android:layout_marginBottom="212dp"
        android:background="@drawable/budesign"
        android:ems="10"
        android:hint="Location"
        android:inputType="textPersonName"
        android:paddingLeft="20dp"
        android:paddingBottom="4dp"
        android:textSize="16dp" />


    <EditText

        android:id="@+id/editText6j"
        android:layout_width="269dp"
        android:layout_height="39dp"
        android:layout_alignStart="@+id/editText4"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="3dp"
        android:layout_marginBottom="158dp"
        android:background="@drawable/budesign"
        android:ems="10"
        android:gravity="center_vertical"
        android:hint="password"
        android:inputType="textPassword"
        android:paddingLeft="20dp"
        android:paddingBottom="4dp"
        android:textSize="16dp"
        />
    <Button
        android:id="@+id/GoUser"
        android:layout_width="157dp"
        android:layout_height="38dp"
        android:layout_alignStart="@+id/textView6"
        android:layout_alignTop="@+id/editText6j"
        android:layout_marginStart="-13dp"
        android:layout_marginTop="66dp"
        android:background="@drawable/budesign2"
        android:textColor="@color/colorPrimary"
        android:textSize="20dp"
        android:text="Go"

        />
</RelativeLayout>